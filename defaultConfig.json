{
  "offsets": {
    "default_add1Day": {
      "func": "add",
      "args": [1, "day"]
    },
    "default_add2Days": {
      "func": "add",
      "args": [2, "days"]
    },
    "default_add1Week": {
      "func": "add",
      "args": [1, "week"]
    },
    "default_startOfDay": {
      "func": "startOf",
      "args": ["day"]
    },
    "default_startOfWeek": {
      "func": "startOf",
      "args": ["week"]
    }
  },
  "timeStamps": {
    "default_now": {
      "base": "now"
    },
    "default_tomorrowStart": {
      "base": "now",
      "offsets": [
        "default_add1Day",
        "default_startOfDay"
      ]
    },
    "default_now+2dStart": {
      "base": "now",
      "offsets": [
        "default_add2Days",
        "default_startOfDay"
      ]
    },
    "default_nextWeekStart": {
      "base": "now",
      "offsets": [
        "default_add1Week",
        "default_startOfWeek"
      ]
    }
  },
  "eventBuckets": {
    "default_ongoing": {
      "displayName": "Ongoing",
      "from": "default_now",
      "to": "default_now"
    },
    "default_today": {
      "displayName": "Today",
      "from": "default_now",
      "to": "default_tomorrowStart"
    },
    "default_tomorrow": {
      "displayName": "Tomorrow",
      "from": "default_tomorrowStart",
      "to": "default_now+2dStart"
    },
    "default_thisWeek": {
      "displayName": "This Week",
      "from": "default_now",
      "to": "default_nextWeekStart"
    }
  },
  "views": [
    {
      "id": "default_todayTomorrow",
      "displayName": "Today + Tomorrow",
      "eventBuckets": {
        "buckets": [
          "default_ongoing",
          "default_today",
          "default_tomorrow"
        ],
        "sort": true,
        "exclusive": true
      }
    },
    {
      "id": "default_thisWeek",
      "displayName": "This Week",
      "eventBuckets": {
        "buckets": [
          "default_ongoing",
          "default_today",
          "default_tomorrow",
          "default_thisWeek"
        ],
        "sort": true,
        "exclusive": true
      }
    }
  ],
  "activeView": "default_todayTomorrow"
}